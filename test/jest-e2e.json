{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": "/dist",
  "testEnvironment": "node",
  "testRegex": ".spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper":{
    "^@domain/(.*)$": ["<rootDir>/src/@core/domain/$1"],
    "^@use-cases/(.*)$": ["<rootDir>/src/@core/use-cases/$1"]
  }
}
